{
    "name": "innmind/http-session",
    "type": "library",
    "description": "HTTP session",
    "keywords": ["http", "session"],
    "homepage": "http://github.com/Innmind/HttpSession",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/HttpSession/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/immutable": "^2.8",
        "innmind/http": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\HttpSession\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\HttpSession\\": "tests/",
            "Fixture\\Innmind\\HttpSession\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "giorgiosironi/eris": "^0.10.0"
    }
}
