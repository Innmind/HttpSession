    "keywords": ["http", "session"],
    "homepage": "http://github.com/Innmind/HttpSession",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/HttpSession/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/immutable": "~4.9|~5.0",
        "innmind/http": "~7.0",
        "innmind/validation": "~1.0"
    },
    "autoload": {
